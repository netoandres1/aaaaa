<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.eazykart" >

    <application
        android:allowBackup="true"
        android:theme="@style/MyTheme" >
        <activity
            android:name="example.eazykart.MainActivity"
            android:configChanges="orientation"
            android:icon="@drawable/ic_launcher"
            android:theme="@style/MyTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="example.eazykart.SingleDetailActivity"
            android:label="@string/title_activity_single_detail"
            android:theme="@style/MyTheme" >
        </activity>
        <activity
            android:name="example.eazykart.ReachUs"
            android:label="@string/title_activity_reach_us"
            android:theme="@style/MyTheme">
        </activity>
    </application>

</manifest>
